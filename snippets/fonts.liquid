{% # theme-check-disable %}
{%- comment -%} Custom Google Fonts for Lords Love Butter {%- endcomment -%}
<link rel="preconnect" href="https://fonts.googleapis.com">
<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
<link href="https://fonts.googleapis.com/css2?family=Playfair+Display:wght@400;700&family=Lato:wght@400;600;700&family=Amatic+SC:wght@400;700&display=swap" rel="stylesheet">

{%- unless settings.type_body_font.system? -%}
  <link
    rel="preload"
    as="font"
    href="{{ settings.type_body_font | font_url }}"
    type="font/woff2"
    crossorigin
    fetchpriority="low"
  >
{%- endunless -%}
{%- unless settings.type_subheading_font.system? -%}
  <link
    rel="preload"
    as="font"
    href="{{ settings.type_subheading_font | font_url }}"
    type="font/woff2"
    crossorigin
    fetchpriority="low"
  >
{%- endunless -%}
{%- unless settings.type_heading_font.system? -%}
  <link
    rel="preload"
    as="font"
    href="{{ settings.type_heading_font | font_url }}"
    type="font/woff2"
    crossorigin
    fetchpriority="low"
  >
{%- endunless -%}
{%- unless settings.type_accent_font.system? -%}
  <link
    rel="preload"
    as="font"
    href="{{ settings.type_accent_font | font_url }}"
    type="font/woff2"
    crossorigin
    fetchpriority="low"
  >
{%- endunless -%}
{% # theme-check-enable %}
